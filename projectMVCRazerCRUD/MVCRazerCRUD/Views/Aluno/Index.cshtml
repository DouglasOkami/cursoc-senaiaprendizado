@{ 
    var listaDeAlunos = ViewBag.ListaDeAlunos;
    
}
<h1> Aqui vamos criar nossa página de listagem de alunos</h1>
<table>
    
    <table class="table table-striped table-dark table embed-responsive">
        <th>ID</th>
        <th>Nome</th>
        <th>Email</th>
        <th>Endereço</th>
        <th>Telefone</th>
        <th>Escolaridade</th>
        <th>Ação</th>
        </thead>
        @foreach (var item in listaDeAlunos)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Nome</td>
                <td>@item.Email</td>
                <td>@item.Endereco</td>
                <td>@item.Telefone</td>
                <td>@item.Escolaridade</td>
                <td>
                    <a href="https://localhost:44314/Aluno/Editar/@item.Id">Editar</a>
                    <a href="https://localhost:44314/Aluno/@item.Id">Excluir</a>
                </td>
            </tr>
        }
    </table>