<h1>Página de Cadastro de aluno</h1>
<form action='@Url.Action("Cadastrar")' method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label for="nome">Nome:</label>
        <input type="text" name="alunoNome" placeholder="Ex.Douglas Ferreira" id="nome" class="form-control"/>
    </div>
    <div class="form-group">
        <label for="email">Email:</label>
        <input type="text" name="alunoEmail" placeholder="Ex.douglas@gmail.com" id="email" class="form-control"/>
    </div>
    <div class="form-group">
         <label for="endereco">Endereco:</label>
         <input type="text" name="alunoEndereco" placeholder="Ex. São Paulo" id="endereco" class="form-control"/>
    </div>
    <div class="form-group">
         <label for="telefone">Telefone:</label>
         <input type="text" name="alunoTelefone" placeholder="Ex. (11) 9999-9999" id="telefone" class="form-control"/>
     </div> 
    <div form-group>
             <label for="escolaridade">Escolaridade:</label>
             <input type="text" name="alunoEscolaridade" placeholder="Ex. 1° Ano do ensino Médio" id="escolaridade" />
         </div>
    <button type="submit">Cadastrar</button>
</form>
