<h1>Página de Cadastro de Professor</h1>
<form action='@Url.Action("Index")' method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label for="nome">Nome:</label>
        <input type="text" name="professorNome" placeholder="Ex.Douglas Ferreira" id="nome" class="form-control" />
    </div>
    <div class="form-group">
        <label for="email">Email:</label>
        <input type="text" name="professorEmail" placeholder="Ex.douglas@gmail.com" id="email" class="form-control" />
    </div>
    <div class="form-group">
        <label for="endereco">Endereco:</label>
        <input type="text" name="professorEndereco" placeholder="Ex. São Paulo" id="endereco" class="form-control" />
    </div>
    <div class="form-group">
        <label for="telefone">Telefone:</label>
        <input type="text" name="professorTelefone" placeholder="Ex. (11) 9999-9999" id="telefone" class="form-control" />
    </div>
    <div form-group>
        <label for="cargo">Cargo:</label>
        <input type="text" name="professorCargo" placeholder="Ex. Professor de Matemática" id="cargo" class="form-control" />
    </div>
    <button type="submit">Cadastrar</button>
</form>
